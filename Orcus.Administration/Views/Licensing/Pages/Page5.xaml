<UserControl x:Class="Orcus.Administration.Views.Licensing.Pages.Page5"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:steps="clr-namespace:Orcus.Administration.Views.Licensing.Steps"
             d:DataContext="{d:DesignInstance steps:Step5}"
             d:DesignHeight="550"
             d:DesignWidth="390"
             mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Theme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <StackPanel Margin="0,10,0,0" Orientation="Vertical">
        <TextBlock Margin="10,0"
                   Foreground="{StaticResource BlackBrush}"
                   Text="{DynamicResource VerifyingLicense}" />
        <controls:MetroProgressBar Margin="0,5,0,0"
                                   Background="#FF124161"
                                   Foreground="#0899fb"
                                   IsIndeterminate="True" />
    </StackPanel>
</UserControl>