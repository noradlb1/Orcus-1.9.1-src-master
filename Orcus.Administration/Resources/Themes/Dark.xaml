    <SolidColorBrush x:Key="CodeBackgroundBrush" Color="#FF404040" />

    <SolidColorBrush x:Key="TabItemBorderBrush" Color="#484848" />
    <SolidColorBrush x:Key="TabItemMouseOverBorderBrush" Color="#376088" />
    <SolidColorBrush x:Key="TabItemSelectedBackgroundBrush" Color="{StaticResource WhiteColor}" />

    <SolidColorBrush x:Key="TabItemBackgroundBrush" Color="#2B2E2E" />
    <SolidColorBrush x:Key="TabItemMouseOverBackgroundBrush" Color="#353535" />

    <SolidColorBrush x:Key="CpuChartCrossBrush" Color="#4CD9EAF4" />
    <SolidColorBrush x:Key="CpuChartFill" Color="#43117DBB" />
    <SolidColorBrush x:Key="MemoryChartCrossBrush" Color="#4CECDEF0" />
    <SolidColorBrush x:Key="MemoryChartFill" Color="#639528B4" />
    <SolidColorBrush x:Key="EthernetChartCrossBrush" Color="#4CEEDECF" />
    <SolidColorBrush x:Key="EthernetChartFill" Color="#79FFAB1F" />

    <SolidColorBrush x:Key="ConsoleBackgroundBrush" Color="#FF191919" />

    <Color x:Key="DialogBottomColor">#FF2B2B2B</Color>
    <SolidColorBrush x:Key="DialogBottomBrush" Color="{StaticResource DialogBottomColor}" />
    <Color x:Key="WindowButtonColor">#3c3c3c</Color>

    <!--  Visual Studio Icons  -->
    <SolidColorBrush x:Key="HaloBrush" Color="#252525" />
    <SolidColorBrush x:Key="LightBrush1" Color="#FF2A272D" />
    <SolidColorBrush x:Key="LightBrush2" Color="#38343b" />
    <SolidColorBrush x:Key="DarkBrush1" Color="#c5c5c5" />
    <SolidColorBrush x:Key="DarkBrush2" Color="#FFC5C5C5" />
    <SolidColorBrush x:Key="IconBlueBrush1" Color="#74BEFF" />
    <SolidColorBrush x:Key="IconBlueBrush2" Color="#75beff" />
</ResourceDictionary>